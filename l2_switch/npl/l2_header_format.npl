

struct snap_llc_t {
    fields {
        bit[48]     snap_llc;
    }
}

struct snap_t {
    fields {
        bit[48]     oui;
        bit[16]     ethertype;
    }
}

struct llc_t {
    fields {
        bit[24]     llc;
    }
}

struct vlan_t {
    fields {
        bit[3]      pcp;
        bit[1]      cfi;
        bit[12]     vid;
        bit[16]     ethertype;
    }
}

struct l2_t {
    fields {
        bit[48]     macda;
        bit[48]     macsa;
        bit[16]     ethertype;
    }
}


